<?xml version="1.0"?>
<Container version="2">
  <Name>infrared</Name>
  <Repository>haveachin/infrared:latest</Repository>
  <Registry>https://registry.hub.docker.com/r/haveachin/infrared</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/haveachin/infrared/issues</Support>
  <Project/>
  <Overview>Infrared, The Reverse Proxy for Minecraft</Overview>
  <Category>GameServers:</Category>
  <WebUI/>
  <Icon>https://github.com/haveachin/infrared/raw/main/docs/public/img/logo.svg</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <Requires/>
  <Config Name="Port" Target="25565" Default="25565" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">25565</Config>
  <Config Name="Data" Target="/etc/infrared" Default="/mnt/cache/docker/infrared" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"></Config>
</Container>
